	<!-- template模板 -->
	<script id="tpl" type="text/x-handlebars-template">			
				{{#each this}}
					<tr>
						<td>{{id}}</td>
						<td><a href="details?id={{id}}" style="color:#000;" >{{fund_shortName}}</a></td>
						<td id="fundCompany">{{fundCompany}}</td>
						<td class="red">{{nav}}%</td>
						<td class="red">{{cumulative_nav}}%</td>
						<td class="red">{{topOneMonth}}%</td>
						<td>{{annualized}}</td>
						<td>{{sharpeatio}}</td>
						<td>{{oneyearrating}}</td>	
						<td><a style="color:#000;" href="details?id={{id}}" >了解详情</a></td>		
					</tr>
				{{/each}}
	</script>
	
	
