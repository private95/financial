#link-info .icon-user{
    display: inline-block;
    width: 13px;
    height: 13px;
    background-image: url("../images/common/gr_top.png");
}
#detailLink{
    position: absolute;
    right: -15px;
    width: 110px;
    text-align: center;
    padding: 10px;
    display: none;

}
#detailLink:before{
    content: " ";
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 0  10px 10px ;
    position: absolute;
    border-color: transparent  transparent #ffffff ;
    left: 50%;
    top: 0;
    margin-left: -6px;
}
#detailLink>ul{
    background-color: #ffffff;
    border-radius: 3px;
    z-index: 10;
}
#detailLink>ul>li{
    padding: 5px 8px;
}
#detailLink>ul a{
    display: inline-block;
    width: 100%;
    color: #333333;
    font-size: 12px;
    line-height: 30px;
    border-bottom: 1px dashed #CCCCCC;
}
#detailLink>ul a:hover{
    color: #DA6017;

}
#detailLink>ul li:last-child a{
    border-bottom: none;
}
#personal{
    position: relative;
}
#personal:hover #detailLink{
    display: block;
}
#link-info .member-name{
    display: inline-block;
    margin-left: 5px;
    font-size: 14px;
}

#top-main .search-main{
    display: inline-block;
    width: 365px;
    position: relative;
}
#top-main .search-main input{
    width: 100%;
    padding-left: 10px;
    box-sizing: border-box;
    height: 35px;
    outline:none;
    border-bottom-color:#9A9A9A;
    background:none transparent !important;
    color: #ffffff;
}
#top-main .search-main a{
    display: inline-block;
    width: 47px;
    height: 35px;
    background-image: url("../images/recommend/sousuo.jpg");
    position: absolute;
    top: 3px;
    right: 0;
}

#header .groom{
    position: relative;
    top: -80px;
    left: 0;
}
#header .groom>p{
    font-size: 20px;
    color: #ffffff;
    margin-bottom: 10px;
}
